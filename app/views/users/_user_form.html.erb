<%= form_for @user do |f| %>
	<%= render "shared/error_messages", object: f.object %>
	<%= f.label :name %>
	<%= f.text_field :name, class: "form-control" %><br>
	<%= f.label :email %>
	<%= f.email_field :email, class: "form-control" %><br>
	<%= f.submit "Sign Up", class: "btn btn-primary" %>
<% end %>